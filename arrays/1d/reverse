#include<iostream>
using namespace std;

int main()
{
	int n,i;
	cout<<"enter array size";
	cin>>n;
	int ip[n];
	int op[n];
	
	cout<<"enter the elements";
	
	for(i=0;i<n;i++)
	{
		cin>>ip[i];
	}
	
	for ( i=0;i<n;i++)
	{
		op[i]=ip[n-i-1];
		}	
		
    cout<<"reversed array is:";
    for(i=0;i<n;i++)
    {
    	cout<<op[i]<<" ";
	}

	return 0;
	
}
